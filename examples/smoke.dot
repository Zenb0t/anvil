digraph test_pipeline {
  graph [goal="Create a hello world Python script"]
  rankdir=LR

  start     [shape=Mdiamond, label="Start"]
  plan      [shape=box, prompt="Plan implementation for: $goal"]
  implement [shape=box, prompt="Write the code", goal_gate=true]
  review    [shape=box, prompt="Review the implementation"]
  done      [shape=Msquare, label="Done"]

  start -> plan -> implement -> review -> done
}
