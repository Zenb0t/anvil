{
  "name": "attractor-cxdb-factory",
  "version": "1.0.0",
  "private": true,
  "description": "Factory bootstrap with Attractor specs persisted into CXDB",
  "scripts": {
    "factory:init": "bun run src/factory-cli.ts init",
    "factory:status": "bun run src/factory-cli.ts status",
    "factory:task": "bun run src/factory-cli.ts task",
    "attractor:lint": "bun run src/attractor-cli.ts lint",
    "attractor:run": "bun run src/attractor-cli.ts run",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "^1.2.0",
    "typescript": "^5.7.0"
  }
}
